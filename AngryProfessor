   public static string angryProfessor(int k, List<int> a)
    {
        int countOfStudArraivedOnTime = 0;
        //foreach(int listOfStud in a){
        //    if(listOfStud <= 0){
        //        countOfStudArraivedOnTime++;
        //    }
        //}
        int i = a.Count(x => x <= 0);
        if(i >= k){
            return "NO";
        }
        else{
            return "YES";
        }
    }
